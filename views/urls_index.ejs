<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <header>
    <%- include partials/_header.ejs %>
  </header>
    <!-- Add your EJS code here -->
  <ul>
    <% for (key in urls) { %>
    <li><%= `${key}: ${urls[key]}` %>

      <a href="/urls/<%=key%>">Edit</a>

      <form method="POST" action="/urls/<%=key%>/delete">
        <button>Delete</button>
      </form></li>

    <% }; %><br>

    <a href="#">Placeholder link</a>

  </ul>
</body>
</html>